<ul>
  <%- if controller_name != 'sessions' %>
    <li><%= link_to "Sign in", new_session_path(resource_name) %></li>
  <% end -%>

  <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
    <li>
      <%= link_to "Sign up", new_registration_path(resource_name), class: 'mt' %>
    </li>
  <% end -%>
</ul>